   "source": [
    "def recursiveC(n,k):\n",
    "    # base cases \n",
    "    if k == 0: \n",
    "        return 1 \n",
    "    if k == 1:\n",
    "        return n\n",
    "    if n < k:\n",
    "        return 0\n",
    "    # recursive cases \n",
    "    return recursiveC(n-1,k-1)"
   ]
  }
